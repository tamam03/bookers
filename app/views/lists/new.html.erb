<h1>Books</h1>
<p>Find me in app/views/lists/new.html.erb</p>

<h2>New book</h2>

 <!--form_with部分    createアクション-->
<%= form_with model: @list,url:'/lists',method::post do |f| %>
  <h4>Title</h4>
  <%= f.text_field :title %>

  <h4>Body</h4>
  <%= f.text_area :body %>

  <%= f.submit 'Create Book' %>
<% end %>
