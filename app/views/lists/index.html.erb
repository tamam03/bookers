<h1>一覧</h1>
<% @books.each do |book| %>

<h5>Title</h5>
<span><%= book.title %></span>

<h5>Body</h5>
<span><%= book.body %></span>



<%= link_to "Show",list_path(book.id) %>  <%= link_to "Edit",edit_list_path(book.id) %>  <%= link_to "Destroy",destroy_list_path(book.id),method: :delete,"data-confirm" => "Are you sure?" %>

<% end %>
